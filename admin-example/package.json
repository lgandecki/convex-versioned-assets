    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-plugin": "^1.154.12",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv": "^17.2.3",
